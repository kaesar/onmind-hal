{
  "name": "copyparty-service",
  "description": "Copyparty file sharing service",
  "image": "copyparty/ac:latest",
  "commands": {
    "install": [
      "docker pull copyparty/ac:latest"
    ],
    "setup": [
      "mkdir -p ~/wsconf",
      "mkdir -p ~/wsdata/copyparty"
    ],
    "run": "docker run -d --name copyparty --network {{networkName}} -p 3923:3923 -v ~/wsdata/copyparty:/mnt -v ~/wsconf:/cfg copyparty/ac:latest --cfg-vol /cfg --listen 0.0.0.0 3923"
  },
  "variables": ["networkName", "domain"],
  "ports": ["3923:3923"],
  "volumes": [
    "~/wsdata/copyparty:/mnt",
    "~/wsconf:/cfg"
  ],
  "accessUrl": "https://files.{{domain}}",
  "dependencies": [],
  "isCore": true
}