{
  "name": "Draw.io Diagramming Tool",
  "description": "Web-based diagramming application",
  "commands": {
    "install": [
      "docker pull jgraph/drawio:latest"
    ],
    "setup": [
      "docker network create {{NETWORK_NAME}} || true"
    ],
    "run": "docker run -d --name drawio --network {{NETWORK_NAME}} -p 8088:8080 jgraph/drawio:latest"
  },
  "variables": ["NETWORK_NAME"],
  "dependencies": []
}