{
  "name": "Draw.io Diagramming Tool",
  "description": "Web-based diagramming application",
  "commands": {
    "install": [
      "docker pull jgraph/drawio:latest"
    ],
    "setup": [
      "docker network create {{networkName}} || true"
    ],
    "run": "docker run -d --name drawio --network {{networkName}} -p 8088:8080 jgraph/drawio:latest"
  },
  "variables": ["networkName"],
  "dependencies": []
}