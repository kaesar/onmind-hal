{
  "name": "Uptime Kuma Monitoring",
  "description": "Self-hosted uptime monitoring tool",
  "commands": {
    "install": [
      "docker pull louislam/uptime-kuma:2"
    ],
    "setup": [
      "docker volume create uptime_kuma_data"
    ],
    "run": "docker run -d --name uptimekuma --network {{NETWORK_NAME}} -p 3003:3001 -v uptime_kuma_data:/app/data --restart=always louislam/uptime-kuma:2"
  },
  "variables": ["NETWORK_NAME"],
  "dependencies": []
}