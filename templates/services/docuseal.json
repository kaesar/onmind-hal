{
  "name": "DocuSeal Document Signing Platform",
  "description": "Open-source document signing and PDF form filling platform",
  "commands": {
    "install": [
      "docker pull docuseal/docuseal:latest"
    ],
    "setup": [
      "docker network create {{networkName}} || true",
      "mkdir -p ~/wsdata/docuseal"
    ],
    "run": "docker run -d --name docuseal --network {{networkName}} -p 3002:3000 -v ~/wsdata/docuseal:/data docuseal/docuseal:latest"
  },
  "variables": ["networkName"],
  "dependencies": []
}